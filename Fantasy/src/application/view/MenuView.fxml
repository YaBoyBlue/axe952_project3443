<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.Cursor?>

<AnchorPane fx:id="anchorPane" maxHeight="800.0" maxWidth="600.0" minHeight="800.0" minWidth="600.0" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MenuController">
   <children>
      <ImageView fx:id="background" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER" maxHeight="800.0" maxWidth="600.0" minHeight="800.0" minWidth="600.0" prefHeight="800.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Text fx:id="telemetry" strokeType="OUTSIDE" strokeWidth="0.0" text="Telemetry">
               <font>
                  <Font size="24.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Text>
            <Button fx:id="launch" focusTraversable="false" mnemonicParsing="false" text="Launch" />
            <Button fx:id="ship" focusTraversable="false" mnemonicParsing="false" text="Ship" />
            <Button fx:id="abort" focusTraversable="false" mnemonicParsing="false" text="Abort" />
         </children>
      </VBox>
   </children>
</AnchorPane>
